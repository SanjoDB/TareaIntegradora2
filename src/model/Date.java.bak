package model;

public class Date{
	private int day, month, year;
	
	public Date(int pDay, int pMonth, int pYear){
		
		day= pDay;
		month= pMonth;
		year= pYear;
		
	}
	
	public int getDay(){
		
		return day;
		
	}
	
	public int getMonth(){
		
		return month;
		
	}
	
	public int getYear(){
		
		return year;
		
	}
	
	public void setDay(int pDay){
		
		day= pDay;
		
	}
	
	public void setMonth(int pMonth){
		
		month= pMonth;
		
	}
	
	public void setYear(int pYear){
		
		year= pYear;
		
	}
}